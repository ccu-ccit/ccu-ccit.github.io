          class=\"card-delete\"\n              shape=\"circle\"\n              icon=\"delete\"\n              @click=\"() => showDeleteConfirm(activityObj)\"\n            />\n          </a-card>\n        </a-col>\n        <a-col :span=\"8\" style=\"margin-bottom: 80px\">\n          <a-card :bordered=\"false\">\n            <div class=\"ant-card-body\" style=\"display: flex\">\n              <a-icon\n                style=\"font-size: 30px\"\n                type=\"plus-circle\"\n                @click=\"() => openEditModal({})\"\n              />\n            </div>\n          </a-card>\n        </a-col>\n      </a-row>\n    </div>\n  </div>\n</template>\n<script>\nimport mngmtActivity from \"./MngmtActivity\";\nimport selectQuestion from \"./SelectQuestion\";\n\n\nexport default {\n  name: \"Home\",\n  components: {\n    \"mngmt-activity\": mngmtActivity,\n    \"select-question\": selectQuestion,\n  },\n  data() {\n    return {\n      visible: false,\n      editorVisible: false,\n      selectedActivity: {},\n    };\n  },\n  computed: {\n    activities(){\n      return this.$store.state.activities;\n    }\n  },\n  created() {\n    console.log(this.activities);\n  },\n  methods: {\n    openModal(activity) {\n      this.selectedActivity = activity;\n      this.visible = !this.visible;\n    },\n    openEditModal(activity) {\n      this.selectedActivity = activity;\n      this.editorVisible = !this.editorVisible;\n    },\n    handleOk() {\n      this.$router.push({ name: \"activity-code\" });\n    },\n    showDeleteConfirm() {\n      this.$confirm({\n        title: \"確定要刪除此活動嗎?\",\n        okText: \"Yes\",\n        okType: \"danger\",\n        cancelText: \"No\",\n        onOk() {\n          console.log(\"OK\");\n        },\n        onCancel() {\n          console.log(\"Cancel\");\n        },\n      });\n    },\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n.card-editor {\n  position: absolute;\n  right: 20px;\n  bottom: 15px;\n  font-size: 20px;\n}\n\n.card-delete {\n  position: absolute;\n  right: 60px;\n  bottom: 15px;\n  font-size: 20px;\n}\n.ant-card-body {\n  align-items: center;\n  justify-content: center;\n  min-height: 200px;\n}\n</style>"],"sourceRoot":"src/components/teacher"}]}