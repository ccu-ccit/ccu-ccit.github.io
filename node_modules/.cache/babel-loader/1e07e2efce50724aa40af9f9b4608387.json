-empty v-if=\"questions.length === 0\" style=\"margin: 15px\"></a-empty>\n      <template v-else>\n        <a-card\n          v-for=\"question in questions\"\n          :key=\"question.serial\"\n          :title=\"question.question\"\n          :bordered=\"false\"\n          class=\"thumbnail\"\n          style=\"margin: 15px\"\n          @click=\"edit(question)\"\n        >\n          <a-radio-group\n            name=\"radioGroup1\"\n            disabled\n            :default-value=\"1\"\n            style=\"width: 80%\"\n            v-model=\"question.answer\"\n            size=\"small\"\n          >\n            <a-radio :value=\"1\"> A. {{ question.options[\"1\"] }}</a-radio>\n            <a-radio :value=\"2\"> B. {{ question.options[\"2\"] }}</a-radio>\n            <a-radio :value=\"3\"> C. {{ question.options[\"3\"] }}</a-radio>\n            <a-radio :value=\"4\"> D. {{ question.options[\"4\"] }}</a-radio>\n          </a-radio-group>\n          <a slot=\"extra\">\n            <a-icon style=\"color: gray\" type=\"close\" />\n          </a>\n        </a-card>\n      </template>\n    </a-layout-sider>\n    <a-layout>\n      <div class=\"main-panel\">\n        <a-layout-header>\n          <a-input\n            placeholder=\"題目名稱\"\n            class=\"title\"\n            v-model=\"selectedQuestion.question\"\n          />\n        </a-layout-header>\n        <a-layout-content style=\"margin: 40px 0px\">\n          <a-radio-group\n            name=\"radioGroup\"\n            :default-value=\"1\"\n            style=\"width: 100%\"\n            v-model=\"selectedQuestion.answer\"\n          >\n            <div>\n              <a-col class=\"gutter-row\" :span=\"12\">\n                <div class=\"gutter-box\">\n                  <a-radio :value=\"1\">\n                    A. <a-input placeholder=\"選項a\" v-model=\"options[1]\" />\n                  </a-radio>\n                </div>\n              </a-col>\n              <a-col class=\"gutter-row\" :span=\"12\">\n                <div class=\"gutter-box\">\n                  <a-radio :value=\"2\">\n                    B. <a-input placeholder=\"選項b\" v-model=\"options[2]\" />\n                  </a-radio>\n                </div>\n              </a-col>\n            </div>\n            <div style=\"margin: 50px 0px\">\n              <a-col class=\"gutter-row\" :span=\"12\">\n                <div class=\"gutter-box\">\n                  <a-radio :value=\"3\">\n                    C. <a-input placeholder=\"選項c\" v-model=\"options[3]\" />\n                  </a-radio>\n                </div>\n              </a-col>\n              <a-col class=\"gutter-row\" :span=\"12\">\n                <div class=\"gutter-box\">\n                  <a-radio :value=\"4\">\n                    D. <a-input placeholder=\"選項d\" v-model=\"options[4]\" />\n                  </a-radio>\n                </div>\n              </a-col>\n            </div>\n          </a-radio-group>\n        </a-layout-content>\n        <a-layout-footer>\n          <a-button size=\"small\" icon=\"delete\" @click=\"clear\">清除</a-button>\n          <a-button\n            size=\"small\"\n            type=\"primary\"\n            icon=\"save\"\n            style=\"margin: 20px\"\n            @click=\"addQuiz\"\n            :disabled=\"formVaild\"\n            >儲存</a-button\n          >\n        </a-layout-footer>\n      </div>\n    </a-layout>\n  </a-layout>\n</template>\n<script>\nexport default {\n  name: \"mngmtActivity\",\n  props: {\n    activity: {\n      type: Object,\n      default: null,\n    },\n  },\n  created() {\n    console.log(this.activity);\n  },\n  data() {\n    return {\n      index: 0,\n      selectedQuestion: {},\n      question: \"\",\n      labelCol: { span: 4 },\n      wrapperCol: { span: 14 },\n      form: this.$form.createForm(this, { name: \"coordinated\" }),\n    };\n  },\n  computed: {\n    formVaild() {\n      return Object.values(this.questions).includes(\"\");\n    },\n    questions() {\n      return this.activity.questions ? this.activity.questions : [];\n    },\n    options() {\n      if (this.selectedQuestion.options) {\n        return this.selectedQuestion.options;\n      } else {\n        return { 1: \"\", 2: \"\", 3: \"\", 4: \"\" };\n      }\n    },\n  },\n  methods: {\n    detail() {\n      console.log(\"show detail\");\n    },\n    edit(question) {\n      // console.log(quiz)\n      this.selectedQuestion = question;\n    },\n    addQuiz() {\n      let _index = this.quizzes\n        .map((e) => {\n          return e.index;\n        })\n        .indexOf(this.index);\n      console.log(_index);\n      if (_index !== -1) {\n        let quiz = Object.assign({}, this.quiz);\n        this.quizzes[_index] = quiz;\n        this.clear();\n        return;\n      }\n      this.quiz.index = this.index;\n      let quiz = Object.assign({}, this.quiz);\n      console.log(this.quizzes);\n      this.quizzes.push(quiz);\n      this.index += 1;\n      this.clear();\n    },\n    clear() {\n      Object.keys(this.quiz).map((key) => {\n        if (key === \"answer\") {\n          this.quiz[key] = 1;\n        } else if (key !== \"index\") {\n          this.quiz[key] = \"\";\n        }\n      });\n    },\n    remove() {\n      this.quizzes.pop();\n    },\n    destroyed() {\n      console.log(\"destroyed\");\n    },\n  },\n};\n</script>\n<style lang=\"scss\">\n.thumbnail {\n  .ant-card-head {\n    min-height: unset !important;\n    padding: 0 2px !important;\n  }\n  .ant-card-head-title {\n    padding: 2px 0 !important;\n    font-size: 12px;\n  }\n  .ant-card-body {\n    padding: 0 15px !important;\n  }\n}\n.mngmt-activity {\n  .ant-layout-footer {\n    background: unset !important;\n  }\n\n  .ant-layout-header {\n    background: #1a62a7 !important;\n    height: unset !important;\n    border-radius: 10px;\n  }\n  .ant-layout-sider {\n    background: #ececec !important;\n  }\n  .ant-card-extra {\n    padding: 5px 0;\n  }\n  .main-panel {\n    text-align: center;\n    height: 10